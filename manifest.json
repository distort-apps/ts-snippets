{
  "manifest_version": 3,
  "name": "ts-snippets",
  "version": "1.0.1",
  "description": "A chrome extension to save, run, and manage TypeScript snippets",
  "icons": {
    "16": "heart.png",
    "48": "heart.png",
    "128": "heart.png"
  },
  "action": {
    "default_icon": {
      "16": "heart.png",
      "48": "heart.png",
      "128": "heart.png"
    },
    "default_title": "ts-snippets",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "background",
    "sidePanel"
  ],
  "options_page": "options/options.html",
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools/devtools.html"
}
